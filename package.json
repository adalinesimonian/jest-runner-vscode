{
  "name": "jest-runner-vscode",
  "version": "2.0.0",
  "description": "Jest runner for testing VS Code extensions",
  "author": {
    "name": "Adaline Simonian",
    "email": "adalinesimonian@gmail.com",
    "url": "https://github.com/adalinesimonian"
  },
  "license": "ISC",
  "homepage": "https://github.com/adalinesimonian/jest-runner-vscode#readme",
  "repository": "https://github.com/adalinesimonian/jest-runner-vscode",
  "bugs": "https://github.com/adalinesimonian/jest-runner-vscode/issues",
  "funding": "https://github.com/sponsors/adalinesimonian",
  "main": "dist/runner.js",
  "types": "dist/runner.d.ts",
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "tsc --build --clean",
    "format": "run-s \"format:*\"",
    "format:eslint": "eslint --ext .js,.ts --fix .",
    "format:prettier": "prettier --write .",
    "lint": "run-s \"lint:*\"",
    "lint:eslint": "eslint --ext .js,.ts .",
    "lint:prettier": "prettier --check .",
    "lint:types": "tsc --noEmit",
    "prepack": "yarn build",
    "test": "node clean-e2e.mjs && jest -i",
    "ci:download-vscode": "node ci-download-vscode.mjs"
  },
  "packageManager": "yarn@3.1.1",
  "engines": {
    "vscode": ">=1.56.0",
    "node": ">=14.16.0"
  },
  "dependencies": {
    "@jest/core": "^27.2.1",
    "@vscode/test-electron": "^1.6.2",
    "cosmiconfig": "^7.0.1",
    "jest-cli": "^27.2.1",
    "jest-environment-node": "^27.2.0",
    "node-ipc": "^9.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.13",
    "@types/jest": "27.0.3",
    "@types/node": "14.14.45",
    "@types/node-ipc": "^9.2.0",
    "@types/vscode": "1.56.0",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "eslint": "8.4.0",
    "eslint-config-prettier": "8.3.0",
    "execa": "5.1.1",
    "fast-glob": "3.2.7",
    "fs-extra": "10.0.0",
    "jest": "27.4.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "ts-jest": "27.1.0",
    "typescript": "4.5.2"
  }
}
